{
  "name": "dymport",
  "version": "0.1.1",
  "description": "Dynamic module import. Javascript ESM module.",
  "type": "module",
  "engines": {
    "node": ">=v16.13.1 <17",
    "npm": "~8.3.0"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-html": "npx eslint . --ext .js -o ./eslint-html/index.html -f html",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "assertions": "cd ./assertions/ && node ./assertions.js",
    "assertion0": "cd ./assertions/ && node ./assertions.js assertion0"
  },
  "dependencies": {
    "oftypes": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-plugin-jsdoc": "^37.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/simonedelpopolo/dymport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/dymport.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/dymport/issues"
  },
  "keywords": [
    "dynamic import",
    "file",
    "string",
    "remote"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "license": "Apache-2.0"
}
